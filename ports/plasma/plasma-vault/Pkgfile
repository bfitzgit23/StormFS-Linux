# Depends on: kitemmodels libksysguard libplasma networkmanager-qt

name=plasma-vault
version=6.3.4
release=1
source=(https://download.kde.org/stable/plasma/$version/$name-$version.tar.xz)
build_opt="-D CMAKE_INSTALL_PREFIX=$KF6_PREFIX 
             -D CMAKE_INSTALL_LIBEXECDIR=libexec 
             -D CMAKE_BUILD_TYPE=Release         
             -D CMAKE_QT6=ON				
             -D BUILD_QT5=OFF                    
             -D BUILD_TESTING=OFF                
             -W no-dev .."
pre_build() {
      sed -e '/QFuture/i #include <QDebug>' \
               -i kded/engine/commandresult.h
}
