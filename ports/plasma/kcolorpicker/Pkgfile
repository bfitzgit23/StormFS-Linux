# Depends on: cmake qt6

name=kcolorpicker
version=0.3.1
release=1
source=(https://github.com/ksnip/kColorPicker/archive/v${version}/kColorPicker-${version}.tar.gz)
build_opt=" -DCMAKE_INSTALL_PREFIX=/usr 
      -DCMAKE_BUILD_TYPE=Release  
      -DBUILD_SHARED_LIBS=ON      
      -DBUILD_WITH_QT6=ON         "
