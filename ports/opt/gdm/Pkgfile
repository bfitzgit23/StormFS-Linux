version=46.2
release=1
source=(https://download.gnome.org/sources/${name}/46/${name}-${version}.tar.xz)
build_opt="      --prefix=/usr       
      		--buildtype=release 
      		-Dgdm-xsession=true 
      		-Drun-dir=/run/gdm"
