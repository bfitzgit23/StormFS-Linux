name=make-ca
version=1.16.1
release=1
source=(https://github.com/lfs-book/make-ca/archive/v$version/$name-$version.tar.gz)


pkg_build() {


make DESTDIR=$PKG install &&
install -vdm755 $PKG/etc/ssl/local
}
